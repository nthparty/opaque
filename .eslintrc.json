{
  "env": {
    "amd": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "commonjs"
  },
  "rules": {
    "no-unused-vars": [
      "error", { 
        "argsIgnorePattern": "_",
        "varsIgnorePattern": "_"
      }
    ]
  }
}
